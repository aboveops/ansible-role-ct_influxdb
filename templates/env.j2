{% if ct_influxdb_auto_setup|bool %}
DOCKER_INFLUXDB_INIT_MODE={{ ct_influxdb_ct_env_docker_influxdb_init_mode }}
DOCKER_INFLUXDB_INIT_USERNAME={{ ct_influxdb_ct_env_docker_influxdb_init_username }}
DOCKER_INFLUXDB_INIT_PASSWORD={{ ct_influxdb_ct_env_docker_influxdb_init_password }}
DOCKER_INFLUXDB_INIT_ORG={{ ct_influxdb_ct_env_docker_influxdb_init_org }}
DOCKER_INFLUXDB_INIT_BUCKET={{ ct_influxdb_ct_env_docker_influxdb_init_bucket }}
{% if ct_influxdb_ct_env_docker_influxdb_init_retention is defined %}
DOCKER_INFLUXDB_INIT_RETENTION={{ ct_influxdb_ct_env_docker_influxdb_init_retention }}
{% endif %}
{% if ct_influxdb_ct_env_docker_influxdb_init_admin_token is defined %}
DOCKER_INFLUXDB_INIT_ADMIN_TOKEN={{ ct_influxdb_ct_env_docker_influxdb_init_admin_token }}
{% endif %}
{% endif %}
# Extra variables from ct_influxdb_ct_env_extra
{% for key, value in ct_influxdb_ct_env_extra.items() %}
{{ key }}={{ value }}
{% endfor %}